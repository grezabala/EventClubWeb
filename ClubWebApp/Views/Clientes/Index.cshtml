@model List<ClubWebApp.Application.Dominio.DTOS.ClientesDto>

<div class="container">
    <h3 class="mb-4 text-center" >CLIENTES</h3>
    <div class="row">
        @foreach (var cliente in Model)
        {
           <div class="col-md-4">
               <div class="card shadow-lg mb-4">
                   <div class="card-body">
                       <h5 class="card-title">@cliente.NombreCompleto</h5>
                        <p class="card-text">Cedula: @cliente.Cedula</p>
                        <p class="card-text">Celular: @cliente.Celular</p>
                        <p class="card-text">Telefono: @cliente.Telefono</p>
                        <p class="card-text">Fecha de Registro: @cliente.FechaIngreso</p>
                        <p class="card-text">Correo Electrónico: @cliente.Email</p>
                        <p class="card-text">Dirección: @cliente.Direccion</p>
                    </div>
               </div>
           </div>
        }
    </div>
</div>